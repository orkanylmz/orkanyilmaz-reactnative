{
  "name": "upayments",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "doctor": "expo doctor",
    "start:staging": "APP_ENV=staging expo start --dev-client",
    "prebuild:staging": "APP_ENV=staging yarn expo prebuild",
    "android:staging": "APP_ENV=staging expo run:android",
    "ios:staging": "APP_ENV=staging expo run:ios",
    "start:production": "APP_ENV=production expo start --dev-client",
    "prebuild:production": "APP_ENV=production yarn expo prebuild",
    "android:production": "APP_ENV=production expo run:android",
    "ios:production": "APP_ENV=production expo run:ios",
    "build:development:ios": "APP_ENV=development eas build --profile development --platform ios",
    "build:development:android": "APP_ENV=development eas build --profile development --platform android ",
    "build:staging:ios": "APP_ENV=staging eas build --profile development --platform ios",
    "build:staging:android": "APP_ENV=staging eas build --profile development --platform android ",
    "build:production:ios": "APP_ENV=production eas build --profile development --platform ios",
    "build:production:android": "APP_ENV=production eas build --profile development --platform android ",
    "web": "expo start --web",
    "test": "jest",
    "test:watch": "yarn test --watch",
    "test:ci": "yarn test --coverage",
    "test:badges": "yarn test:ci  && jest-coverage-badges --input coverage/coverage-summary.json --output __badges__",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "type-check": "tsc  --noemit"
  },
  "dependencies": {
    "@bacons/link-assets": "^1.1.0",
    "@gorhom/bottom-sheet": "^4",
    "@hookform/resolvers": "^2.9.10",
    "@react-native-community/netinfo": "9.3.5",
    "@react-navigation/native": "^6.0.13",
    "@react-navigation/native-stack": "^6.9.1",
    "@shopify/flash-list": "1.3.1",
    "@tanstack/react-query": "^4.16.1",
    "axios": "^0.27.2",
    "expo": "~47.0.5",
    "expo-asset": "~8.6.2",
    "expo-font": "~11.0.1",
    "expo-linear-gradient": "~12.0.1",
    "expo-splash-screen": "~0.17.5",
    "expo-status-bar": "~1.4.2",
    "moti": "^0.21.0",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-hook-form": "^7.39.4",
    "react-native": "0.70.5",
    "react-native-flash-message": "^0.3.1",
    "react-native-gesture-handler": "~2.8.0",
    "react-native-reanimated": "~2.12.0",
    "react-native-redash": "^18.1.0",
    "react-native-safe-area-context": "4.4.1",
    "react-native-screens": "~3.18.0",
    "react-native-web": "~0.18.9",
    "react-navigation": "^4.4.4",
    "tailwind-react-native-classnames": "^1.5.1",
    "zod": "^3.19.1",
    "zustand": "^4.1.4"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@react-native-community/eslint-config": "^3.2.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/jest-native": "^5.3.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/react-native": "^11.5.0",
    "@types/jest": "^29.2.3",
    "@types/react": "~18.0.14",
    "@types/react-native": "~0.70.6",
    "@types/react-test-renderer": "^18.0.0",
    "eslint": "^8.27.0",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "eslint-plugin-tailwindcss": "^3.7.0",
    "eslint-plugin-unicorn": "^44.0.2",
    "eslint-plugin-unused-imports": "^2.0.0",
    "jest": "^29.3.1",
    "jest-coverage-badges": "^1.1.2",
    "jest-environment-jsdom": "^29.3.1",
    "jest-expo": "^47.0.1",
    "prettier": "^2.7.1",
    "react-test-renderer": "^18.2.0",
    "tailwindcss": "^3.2.4",
    "ts-jest": "^29.0.3",
    "typescript": "^4.6.3"
  },
  "private": true
}
